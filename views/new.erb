<div class="container my-5">
    <h1>New Post</h1>
    <form action='/posts' method='POST'>
        Title : 
        <input type='text' name='title' class="form-control mb-1"/>
        Content :
        <input type='text' name='content' class="form-control mb-1"/>
        Rating :
        <input type='number' name='rating' class="form-control mb-1"/>
        Photo :
        <input type='url' name='photo' class="form-control mb-1"/>
        <input type='submit' class="btn btn-primary"/>
    </form>
</div>
